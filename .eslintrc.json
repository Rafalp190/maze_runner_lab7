{
    "extends": "airbnb",
    "rules": {
        "semi": 0,
        "import/extensions": 0,
        "react/destructuring-assignment": 0,
        "no-restricted-globals": 0,
        "global-require": 0,
        "jsx-filename-extension": 0,
        "react/no-access-state-in-setstate": 0,
        "no-else-return": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "react/prop-types": 0,
        "prefer-template": 0,
        "react/jsx-filename-extension": 0,
        "no-undef": 0
    }
  }
  